include ./asset-custom
include ./asset-builder


mixin assets()
  .assets
    fieldset.repeating_assets.fixed-width
      .asset
        - var assetsArray = assets.Assets.filter(obj => { return obj.Category === 'Path' || obj.Category === 'Companion' || obj.Category === 'Deed' })
        +genericBuilder(assetsArray, 'assetList')
        +customAssets
