mixin challengeDice
  tr
    td
      span Challenge Dice: 
    td
      span {{challenge1}} {{challenge2}}

mixin actionBody
  tr
    td
      span Action Score: 
    td
      span {{action}}
  +challengeDice

mixin vowBody
  tr
    td
      span Vow Progress: 
    td
      span {{vow}}
  +challengeDice