include ../../components/oracles/core
include ../../components/oracles/delve

div.sheet-page-container
  input(type='radio' name='attr_character_page' class='sheet-oracles sheet-hide' value='12')
  div.sheet-oracles.sheet-showhide
    h3.sheet-divide &emsp;ORACLES&emsp;
    .sheet-nav-bar
      .sheet-nav-row
        .sheet-nav-child
          label.sheet-page-label
            input(type='radio' name='attr_oracle_page_button' class='sheet-hide sheet-page-button' value='1' checked)
            .sheet-page-button-back CORE
        .sheet-nav-child
          label.sheet-page-label
            input(type='radio' name='attr_oracle_page_button' class='sheet-hide sheet-page-button' value='2')
            div.sheet-page-button-back DELVE
    
    input.sheet-hide.sheet-oracle-core(type='radio' name='attr_oracle_page' value='1' checked)
    .sheet-oracle-core.sheet-showhide
      +coreOracles
    
    input.sheet-hide.sheet-oracle-delve(type='radio' name='attr_oracle_page' value='2')
    .sheet-oracle-delve.sheet-showhide
      +delveOracles